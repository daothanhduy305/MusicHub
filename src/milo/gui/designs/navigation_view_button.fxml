<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER_LEFT" maxHeight="-Infinity" maxWidth="-Infinity"
         minHeight="-Infinity"
         minWidth="-Infinity" spacing="20.0" type="javafx.scene.layout.HBox"
         xmlns="http://javafx.com/javafx/8.0.102">
    <children>
        <Pane fx:id="status" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="200.0"
              style="-fx-border-width: 0;">
            <HBox.margin>
                <Insets/>
            </HBox.margin>
        </Pane>
        <ImageView fx:id="buttonIcon" pickOnBounds="true" preserveRatio="true"/>
        <Label fx:id="buttonTitleLabel" minWidth="-Infinity" text="Label">
            <font>
                <Font name="Segoe UI" size="18.0"/>
            </font>
        </Label>
    </children>
</fx:root>
